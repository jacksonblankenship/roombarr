{
  "name": "roombarr",
  "type": "module",
  "scripts": {
    "build": "rollup --config",
    "dev": "tsx --require dotenv/config ./src/index.ts dotenv_config_path=./.env",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
