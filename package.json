    "node-cron": "^3.0.3",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "remeda": "^2.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "22.5.4",
    "@types/node-cron": "^3.0.11",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.2",
    "rollup": "^4.21.2",
    "tsx": "^4.19.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "engines": {
    "node": "22.8.0"
  }
